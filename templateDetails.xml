				</field>
				<field name="enableSwitcher" type="radio" default="0" label="TPL_JE_CONSTRUCT_PRO_ENABLE_SWITCHER_LABEL" description="TPL_JE_CONSTRUCT_PRO_ENABLE_SWITCHER_DESC">
