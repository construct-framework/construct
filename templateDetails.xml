				<field name="showDiagnostics" type="radio" default="0" label="TPL_JE_CONSTRUCT_PRO_SHOW_DIAGNOSTICS_LABEL" description="TPL_JE_CONSTRUCT_PRO_SHOW_DIAGNOSTICS_DESC">
